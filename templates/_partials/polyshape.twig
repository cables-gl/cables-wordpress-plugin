{% if isImported %}
    <br/>
    <br/>
    <canvas id="glcanvas" width="100px" height="100px"></canvas>

    <script type="text/javascript" src="{{ patchDir }}js/libs.core.min.js"></script>
    <script type="text/javascript" src="{{ patchDir }}js/cables.min.js"></script>

    <script type="text/javascript" src="{{ patchDir }}js/ops.js"></script>

    <script>

        function showError(err)
        {
            alert(err);
        }

        document.addEventListener("DOMContentLoaded", function(event)
        {
            CABLES.patch=new CABLES.Patch(
                {
                    patchFile:'{{ patchDir }}js/{{ shape.patchname }}.json',
                    prefixAssetPath:'{{ patchDir }}/',
                    glCanvasId:'glcanvas',
                    glCanvasResizeToWindow:false,
                    onError:showError
                });
        });

    </script>
{% endif %}
